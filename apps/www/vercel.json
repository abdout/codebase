{
    "rewrites": [
      {
        "source": "/blocks/:path*",
        "destination": "https://block-ivory.vercel.app/:path*"
      },
      {
        "source": "/_next/:path*",
        "destination": "https://block-ivory.vercel.app/_next/:path*",
        "has": [
          {
            "type": "header",
            "key": "referer",
            "value": "/blocks.*"
          }
        ]
      }
    ]
  }